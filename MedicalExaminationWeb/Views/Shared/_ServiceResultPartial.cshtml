@model ServiceResultViewModel

@{
    ViewData["Title"] = "_ServiceResultPartial";
}

<div class="form-group">
    <label class="control-label col-md-12">@Model.Service.Name</label>
    <div class="form-group">
        <label asp-for="@Model.Result" class="control-label col-md-2"></label>
        <div class="col-md-10">
            <input type="text" asp-for="@Model.Result" class="form-control" />
            @Html.ValidationMessageFor(m => Model.Result, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <label asp-for="@Model.TubeNumber" class="control-label col-md-2"></label>
        <div class="col-md-10">
            <input type="text" asp-for="@Model.TubeNumber" class="form-control" />
            @Html.ValidationMessageFor(m => Model.Result, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Workers" class="control-label col-md-2"></label>
        <div class="col-md-10">
            @Html.DropDownListFor(m => m.WorkerId, Model.Workers, new { @class = "form-control" })
        </div>
    </div>
</div>
